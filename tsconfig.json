{
  "include": ["src"],
  "compilerOptions": {
    "target": "ES2017",
    "module": "Node16",
    "moduleResolution": "Node16",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": false,
    "strict": true,
    "alwaysStrict": true,
    "strictFunctionTypes": false,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "removeComments": false,
    "sourceMap": false,
    "declaration": true,
    "outDir": "",
    "declarationDir": ""
  }
}
